public class Administrador {
    
    private int id;
    private String nombre;
    private String cargo;
    private Float datosTrabajador;

    public Administrador(int id, String nombre, String cargo, Float datosTrabajador) {
        this.id = id;
        this.nombre = nombre;
        this.cargo = cargo;
        this.datosTrabajador = datosTrabajador;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public String getCargo() {
        return cargo;
    }

    public void setCargo(String cargo) {
        this.cargo = cargo;
    }

    public Float getDatosTrabajador() {
        return datosTrabajador;
    }

    public void setDatosTrabajador(Float datosTrabajador) {
        this.datosTrabajador = datosTrabajador;
    }

    @Override
    public String toString() {
        return "Administrador{" + "id=" + id + ", nombre=" + nombre + ", cargo=" + cargo + ", datosTrabajador=" + datosTrabajador + '}';
    }
    
}
